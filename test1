local function WinCondition_Init()
	Rule_AddInterval(WinCondition_Check, 3)
	
	local rootPanel = RootPanel:new()
	
	local panel = Panel:new()
		:setLocation(0, 0)
		:setSize(128, 64)
		  
	rootPanel:add(panel)
	
	local button
	button = Button:new('$043d372bfbda460e99c80ead66f64aed:1', 'ModIcons_043d372bfbda460e99c80ead66f64aed_z2')
		:setLocation(16, 16)
		:setSize(128, 64)
		:setCallback(function()
			panel:remove(button)
		end)
		  
	panel:add(button)
end
